{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true,
    "incremental": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "declaration": false,
    "declarationMap": false
  }
}
