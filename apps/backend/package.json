{
  "name": "backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build --entry src/index.ts --minify --target=bun --outfile dist/bundle.js",
    "start": "bun run dist/index.ts",
    "migrate:make": "bun knex -x ts --migrations-directory ./src/db/migrations migrate:make",
    "migrate:latest": "bun knex migrate:latest --knexfile ./src/db/knexfile.ts",
    "migrate:up": "bun knex migrate:up --knexfile ./src/db/knexfile.ts",
    "migrate:down": "bun knex migrate:down --knexfile ./src/db/knexfile.ts",
    "migrate:rollback": "bun knex migrate:rollback --knexfile ./src/db/knexfile.ts",
    "migrate:list": "bun knex migrate:list --knexfile ./src/db/knexfile.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "connect-redis": "^8.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "google-auth-library": "^9.15.1",
    "pg": "^8.13.3",
    "pkce-challenge": "^4.1.0",
    "redis": "^4.7.0"
  }
}
